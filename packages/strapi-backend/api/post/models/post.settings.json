{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "Post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Excerpt": {
      "type": "text"
    },
    "Content": {
      "type": "richtext"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    }
  }
}
