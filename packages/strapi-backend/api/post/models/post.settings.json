{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "Post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Excerpt": {
      "type": "text"
    },
    "Content": {
      "type": "richtext"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Thumbnail": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "Images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    }
  }
}
